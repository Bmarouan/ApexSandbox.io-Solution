public List<apxio__Student__c> selectUnreachableStudents() {
     List<apxio__Student__c> student = [Select id, name, email, apxio__Active__c from apxio__Student__c
                                        where apxio__Active__c = true and 
                                        phone = null and
                                        email = null order by name Asc


     ]; 
     return student;
}
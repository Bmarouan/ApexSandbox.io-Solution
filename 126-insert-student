public Id insertStudent(String name, String email) {
    apxio__Student__c st  = new apxio__Student__c() ;
    st.Name = 'ok';
    st.apxio__Email__c = 'Test';
    insert st;

    apxio__Student__c recordStudent = [Select id, apxio__Registration_Number__c
                                    from apxio__Student__c 
                                    where   apxio__Registration_Number__c =: st.id];
    return recordStudent.apxio__Registration_Number__c;
}